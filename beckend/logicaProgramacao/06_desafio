numeros = [1, 5, 9, 14, 11, 2, 7, 12, 15, 10, 1, 5, 9, 14, 11, 2, 7, 12, 15, 10]

for numero in numeros:
    if numero % 2 == 0:
        print(numero, "é par")
    else:
        print(numero, "é impar")